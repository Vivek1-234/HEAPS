#include<iostream>
#include<queue>
using namespace std;

int main(){

priority_queue<int , vector<int> > pq ; 
pq.push(2);
pq.push(3);    //  this will print all in the decresaing order --->>>>>>>  3 , 2, 1 
pq.push(1);

cout<<pq.top()<<endl;
pq.pop();

cout<<pq.top()<<endl;

cout<<endl;
//  if we want to print in the oncresing order then how we wil print this ---------->>>>>>>...   1 ,2 ,3 -->>>>>>>.  let' s see

priority_queue<int  , vector<int> , greater<int> > pqm ; 
pqm.push(2);
pqm.push(1);
pqm.push(3);

cout<<pqm.top()<<endl;
pqm.pop();

cout<<pqm.top()<<endl;

return 0;

 }
